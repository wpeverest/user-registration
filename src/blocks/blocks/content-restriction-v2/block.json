{
  "$schema": "https://schemas.wp.org/block-json/1.1",
  "apiVersion": 3,
  "name": "user-registration/content-restriction-enhanced-v2",
  "version": "1.0.0",
  "title": "Content Restriction",
  "category": "user-registration",
  "description": "Restrict Gutenberg blocks easily with content restrict rules.",
  "keywords": [
    "user registration content restriction",
    "content restriction"
  ],
  "textdomain": "user-registration",
  "example": {
    "attributes": {}
  },
  "supports": {
    "className": true,
    "customClassName": false,
    "reusable": false
  },
  "attributes": {
    "content": {
      "type": "string",
      "selector": "div"
    },
    "message": {
      "type": "string",
      "default": ""
    },
    "enableContentRestriction": {
      "type": "boolean",
      "default": false
    },
    "contentRule": {
      "type": "number",
      "default": ""
    },
    "enableCustomRestrictionMessage": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "user-registration-blocks-editor",
  "editorStyle": "user-registration-blocks-editor"
}
