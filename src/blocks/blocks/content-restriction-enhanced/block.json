{
  "$schema": "https://schemas.wp.org/block-json/1.1",
  "apiVersion": 3,
  "name": "user-registration/content-restriction-enhanced",
  "version": "1.0.0",
  "title": "Content Restriction",
  "category": "user-registration",
  "description": "A basic wrapper block with partial content restriction capabilities for pages and posts.",
  "keywords": [
    "user registration content restriction",
    "content restriction"
  ],
  "textdomain": "user-registration",
  "example": {
    "attributes": {},
    "innerBlocks": [
      {
        "name": "core/gallery",
        "attributes": {
          "columns": 3,
          "imageCrop": true,
          "linkTo": "none"
        }
      },
      {
        "name": "core/paragraph",
        "attributes": {
          "content": "This block is restricted!!"
        }
      }
    ],
    "viewportWidth": 400
  },
  "supports": {
    "className": false,
    "customClassName": false,
    "reusable": false,
    "inserter": false
  },
  "attributes": {
    "accessAllRoles": {
      "type": "string",
      "default": ""
    },
    "accessSpecificRoles": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "accessMembershipRoles": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "accessControl": {
      "type": "string",
      "default": "access"
    },
    "content": {
      "type": "string",
      "selector": "div"
    },
    "message": {
      "type": "string",
      "default": ""
    },
    "enableContentRestriction": {
      "type": "boolean",
      "default": false
    },
    "restrictionMessageType": {
      "type": "string",
      "default": "custom"
    }
  },
  "editorScript": "user-registration-blocks-editor",
  "editorStyle": "user-registration-blocks-editor"
}
