{
  "name": "user-registration",
  "title": "User Registration",
  "version": "5.0.0",
  "homepage": "https://wpuserregistration.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wpeverest/user-registration.git"
  },
  "license": "GPL-3.0+",
  "main": "Gruntfile.js",
  "scripts": {
    "build": "npx grunt && npm run makepot && wp-scripts build --node-env=production --mode=production",
    "dev": "wp-scripts start --node-env=development --mode=development",
    "makepot": "composer run-script makepot",
    "start": "wp-scripts start --node-env=development --mode=development",
    "eslint-fix": "eslint --fix \"src/**/*.js\" \"assets/js/admin/gutenberg/*.js\"",
    "prelint-php": "wp-env run composer 'install --no-interaction'",
    "lint-php": "wp-env composer run-script phpcs",
    "phpcs": "composer run-script phpcs --timeout 100000",
    "phpcbf": "composer run-script phpcbf --timeout 100000",
    "test-php": "npm run lint-php && npm run test-unit-php",
    "test-php:watch": "wp-env run composer run-script test:watch",
    "pretest-unit-php": "wp-env start",
    "test-unit-php": "wp-env run phpunit 'phpunit --configuration=/var/www/html/wp-content/plugins/user-registration/phpunit.xml.dist --verbose'",
    "phpunit-test": "phpunit --verbose",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "^7.24.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/jquery": "^3.5.29",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/wordpress__block-editor": "^11.5.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@wordpress/block-editor": "^12.12.0",
    "@wordpress/blocks": "^12.29.0",
    "@wordpress/components": "^25.16.0",
    "@wordpress/hooks": "^4.37.0",
    "@wordpress/i18n": "^6.10.0",
    "@wordpress/scripts": "^31.1.0",
    "@wordpress/server-side-render": "^4.30.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "~5.2.1",
    "eslint": "^7.32.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "grunt": "^1.5.3",
    "grunt-contrib-compress": "^1.5.0",
    "grunt-contrib-concat": "~1.0.1",
    "grunt-contrib-cssmin": "~2.2.1",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-contrib-uglify": "~3.2.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-phpcs": "^0.4.0",
    "grunt-postcss": "~0.9.0",
    "grunt-rtlcss": "~2.0.1",
    "grunt-sass": "3.1.0",
    "grunt-shell": "^4.0.0",
    "grunt-stylelint": "^0.18.0",
    "html-webpack-plugin": "^5.5.0",
    "react-refresh": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.52.3",
    "sass-loader": "^12.3.0",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.2",
    "webpackbar": "^7.0.0"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.15",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tinymce/tinymce-react": "^6.1.0",
    "@wordpress/api-fetch": "^5.2.6",
    "@wordpress/data": "^9.22.0",
    "@wordpress/icons": "^11.3.0",
    "chakra-react-select": "^4.10.1",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "css-loader": "^6.5.1",
    "framer-motion": "^5.6.0",
    "lodash.debounce": "4.0.8",
    "makepot": "^0.3.0",
    "node-sass": "^7.0.3",
    "path-parse": "^1.0.7",
    "promise": "^8.1.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "18.2.0",
    "react-grid-system": "^8.1.9",
    "react-icons": "5.2.1",
    "react-player": "2.16.0",
    "react-router-dom": "^6.22.1",
    "react-select": "^5.10.1",
    "style-loader": "^3.3.1"
  },
  "lint-staged": {
    "*.php": [
      "php -d display_errors=1 -l",
      "composer run-script phpcs-pre-commit"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "git add"
    ],
    "!(*.min).js": [
      "eslint --fix",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
